%include 'in_out.asm'
SECTION .data
msg db "Результат: ",0
SECTION .text
global _start
_start:
pop ecx
pop edx
sub ecx,1
mov esi, 0
mov edi,10
mov eax, 5
mul edi
sub eax, 5
add esi, eax
next:
cmp ecx,0h
jz _end
pop eax
mov ebx, 10
mul ebx
sub eax, 5
add esi,eax
loop next
_end:
mov eax, msg
call sprint
mov eax, esi
call iprintLF
call quit